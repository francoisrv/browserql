{
  "name": "browserqlcore",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "c": "bash scripts/command.sh",
    "p": "bash scripts/pkgjson.sh",
    "w": "bash scripts/workspace.sh",
